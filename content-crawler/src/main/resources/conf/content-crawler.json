{
    "name": "Content crawler",
    "crawler": {
        "bizlive.vn": {
            "content": {
                "selector": "div#abody",
                "filter": ".remove(\"div.advan-670\").remove(\"div#keywords\")",
                "return": "html"
            },
            "author": {
                "selector": "div.summary > div.author > p.name > a",
                "filter": "",
                "return": "html"
            },
            "image": {
                "selector": "div.text-content > div > img",
                "filter": "",
                "return": ".attr(\"src\")"
            }
        }
    }
}